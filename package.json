{
  "name": "pyra-4-introduction",
  "private": true,
  "scripts": {
    "dev": "slidev src/slides.md --port 3007 --open",
    "build": "slidev build src/slides.md --out ../dist"
  },
  "dependencies": {
    "@iconify-json/tabler": "^1.1.100",
    "@rive-app/canvas": "^2.25.7",
    "@slidev/cli": "^0.45.0",
    "@slidev/theme-default": "^0.21.2",
    "@slidev/types": "^0.45.0",
    "@vueuse/core": "^12.5.0",
    "codemirror-theme-vars": "^0.1.2",
    "katex": "^0.16.9",
    "markdown-it": "^13.0.2",
    "markdown-it-katex": "^2.0.3",
    "playwright-chromium": "^1.40.1",
    "prism-theme-vars": "^0.2.4",
    "theme-vitesse": "^0.7.5",
    "unplugin-icons": "^0.18.1"
  },
  "slidev": {
    "colorSchema": "light",
    "defaults": {
      "titleTemplate": "%s",
      "theme": ".",
      "layout": "default",
      "exportFilename": "../slides.pdf",
      "fonts": {
        "weights": "100,200,300,400,500,600,700,800,900"
      },
      "aspectRatio": "16/9",
      "css": "windicss"
    }
  },
  "devDependencies": {
    "@types/node": "^22.13.0"
  }
}
